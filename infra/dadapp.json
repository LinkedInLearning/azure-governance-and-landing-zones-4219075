{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "appService_lilDadApp_name": {
      "type": "string"
    },
    "appServicePlan_DadApp-Plan_location": {
      "type": "string"
    },
    "appServicePlan_DadApp-Plan_name": {
      "type": "string"
    },
    "appServicePlan_DadApp-Plan_skuName": {
      "type": "string"
    },
    "appServicePlan_DadApp-Plan_skuTier": {
      "type": "string"
    },
    "appServicePlan_DadApp-Plan_skuFamily": {
      "type": "string"
    },
    "appServicePlan_DadApp-Plan_skuSize": {
      "type": "string"
    },
    "appServicePlan_DadApp-Plan_workerSizeId": {
      "type": "int"
    }
  },
  "resources": [
    {
      "location": "[parameters('appServicePlan_DadApp-Plan_location')]",
      "name": "[parameters('appService_lilDadApp_name')]",
      "type": "Microsoft.Web/sites",
      "apiVersion": "2018-02-01",
      "tags": {
        "[concat('hidden-related:', resourceId('Microsoft.Web/serverFarms', parameters('appServicePlan_DadApp-Plan_name')))]": "empty"
      },
      "dependsOn": [
        "[resourceId('Microsoft.Web/serverFarms', parameters('appServicePlan_DadApp-Plan_name'))]"
      ],
      "kind": "app",
      "properties": {
        "name": "[parameters('appService_lilDadApp_name')]",
        "httpsOnly": true,
        "reserved": false,
        "serverFarmId": "[resourceId('Microsoft.Web/serverFarms', parameters('appServicePlan_DadApp-Plan_name'))]",
        "siteConfig": {
          "ftpsState": "FtpsOnly",
          "linuxFxVersion": "DOTNETCORE|8.0"
        }
      },
      "identity": {
        "type": "SystemAssigned"
      }
    },
    {
      "location": "[parameters('appServicePlan_DadApp-Plan_location')]",
      "name": "[parameters('appServicePlan_DadApp-Plan_name')]",
      "type": "Microsoft.Web/serverFarms",
      "apiVersion": "2018-02-01",
      "kind": "linux",
      "sku": {
        "Name": "[parameters('appServicePlan_DadApp-Plan_skuName')]",
        "Tier": "[parameters('appServicePlan_DadApp-Plan_skuTier')]"
      },
      "properties": {
        "name": "[parameters('appServicePlan_DadApp-Plan_name')]",
        "workerSize": "[parameters('appServicePlan_DadApp-Plan_workerSizeId')]",
        "workerSizeId": "[parameters('appServicePlan_DadApp-Plan_workerSizeId')]",
        "numberOfWorkers": "1",
        "reserved": true
      }
    }
  ]
}